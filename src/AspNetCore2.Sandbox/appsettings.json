{
  "ConnectionStrings": {
    "DefaultConnection": "DataSource=app.db"
  },
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "Loupe": {
    "Publisher": {
      "ProductName": "AspNetCore2.Sandbox",
      "ApplicationName": "Test",
      "ApplicationType": "AspNet",
      "ApplicationVersionNumber": "1.0.1"
    },
    "Server": {
      "UseGibraltarService": true,
      "SendAllApplications": true,
      "Customer": "Your_Customer_Name"
    },
    "Performance": {
      "EnableDiskMetrics": false,
      "EnableNetworkMetrics": false
    } 
  },
  "AllowedHosts": "*"
}
